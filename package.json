{
  "name": "sokoban",
  "private": true,
  "version": "1.0.0",
  "description": "Sokoban implemented in React Redux.",
  "license": "MIT",
  "main": "./src/index.jsx",
  "scripts": {
    "install": "node ./scripts/install.js",
    "start": "cross-env NODE_ENV=development node ./scripts/start.js",
    "test": "npm-run-all jest visual-regression",
    "jest": "jest",
    "visual-regression": "cross-env NODE_ENV=test node ./scripts/test.js",
    "test:update": "npm-run-all jest --updateSnapshot visual-regression:update",
    "visual-regression:update": "cross-env NODE_ENV=test node ./scripts/test.js -- --updateScreenshots",
    "build": "cross-env NODE_ENV=production node ./scripts/build.js",
    "format": "prettier --single-quote --no-semi --write '{,!(node_modules|dist)/**/}*.js*(x)' '{,!(node_modules|dist)/**/}*.css*'",
    "lint": "npm-run-all lint:*",
    "lint:es": "eslint -c .eslintrc --ext .js --ext .jsx .",
    "lint:style": "stylelint -c .stylelintrc 'src/**/*.css'",
    "devtools": "react-devtools"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "rootDir": "./src",
    "setupTestFrameworkScriptFile": "jest-enzyme/lib",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/../configs/setupJest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../mocks/fileMockJest.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "config": {
    "port": 8080
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "brotli-webpack-plugin": "^0.5.0",
    "browserslist": "^2.11.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "download": "^6.2.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "express-ws": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "jest-enzyme": "^4.0.2",
    "minimist": "^1.2.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "npm-run-all": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "pixelmatch": "^4.0.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-sass": "^0.2.0",
    "postcss-url": "^7.3.0",
    "prettier": "^1.9.2",
    "puppeteer": "^0.13.0",
    "raf": "^3.4.0",
    "react-devtools": "^3.0.0",
    "react-hot-loader": "^3.1.3",
    "redux-devtools": "^3.4.1",
    "sinon": "^4.1.3",
    "slug": "^0.9.1",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-no-unsupported-browser-features": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "zopfli-webpack-plugin": "^0.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.2",
    "nskeymirror": "^0.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  }
}
