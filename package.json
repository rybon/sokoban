{
  "name": "sokoban",
  "private": true,
  "version": "1.0.0",
  "description": "Sokoban implemented in React Redux.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rybon/sokoban"
  },
  "main": "./src/index.jsx",
  "scripts": {
    "start": "NODE_ENV=development node ./scripts/start.js",
    "start:production": "NODE_ENV=production node ./scripts/start.js",
    "test": "TZ=GMT jest",
    "test:update": "TZ=GMT jest --updateSnapshot",
    "visual-regression": "NODE_ENV=test node ./scripts/test.js",
    "visual-regression:update": "NODE_ENV=test node ./scripts/test.js --updateScreenshots",
    "build": "NODE_ENV=production node ./scripts/build.js",
    "format": "prettier --list-different '{,!(node_modules|dist)/**/}*.js*(x)' '{,!(node_modules|dist)/**/}*.css*'",
    "lint": "npm-run-all lint:*",
    "lint:es": "eslint --ext .js --ext .jsx .",
    "lint:style": "stylelint -c .stylelintrc 'src/**/*.css'",
    "fix:format": "prettier --write '{,!(node_modules|dist)/**/}*.js*(x)' '{,!(node_modules|dist)/**/}*.css*'",
    "fix:lint": "npm-run-all fix:lint:*",
    "fix:lint:es": "eslint --fix --ext .js --ext .jsx .",
    "fix:lint:style": "stylelint --fix -c .stylelintrc 'src/**/*.css'",
    "devtools": "react-devtools",
    "flow-typed": "flow-typed install --ignoreDeps dev",
    "type-check": "flow",
    "flow:generate-module-name-mappers": "node ./scripts/generateFlowModuleNameMappers.js",
    "styleguide": "styleguidist --config ./configs/styleguide.config.js server",
    "styleguide:build": "styleguidist --config ./configs/styleguide.config.js build",
    "e2e": "start-server-and-test start:production http://localhost:8080 cypress:ci",
    "cypress:open": "TZ=GMT cypress open --project ./e2e",
    "cypress:run": "TZ=GMT cypress run --project ./e2e",
    "cypress:ci": "TZ=GMT cypress run --project ./e2e --record",
    "concatenate": "node ./scripts/concatenate.js",
    "benchmark": "start-server-and-test start:production http://localhost:8080 lighthouse:ci",
    "lighthouse:ci": "node ./scripts/lighthouse.js",
    "lighthouse:open": "lighthouse --config-path=./configs/lighthouse.config.js http://localhost:8080 --view"
  },
  "browserslist": [
    "> 0.5%",
    "not and_uc 11",
    "not op_mini all",
    "not android 4.4"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "lighthouse": {
    "performance": 95,
    "pwa": 100,
    "accessibility": 100,
    "best-practices": 100,
    "seo": 100
  },
  "jest": {
    "testEnvironment": "enzyme",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "rootDir": "./src",
    "setupTestFrameworkScriptFile": "jest-enzyme/lib",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../configs/fileMockJest.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "config": {
    "port": 8080
  },
  "devDependencies": {
    "apollo-server-express": "^1.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-flow-react-proptypes": "^21.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "brotli-webpack-plugin": "^0.5.0",
    "browserslist": "^3.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.2",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.10",
    "cypress": "^2.1.0",
    "download": "^6.2.5",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-jest-enzyme": "^5.0.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.14.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "express-history-api-fallback": "^2.2.1",
    "express-http-to-https": "^1.1.3",
    "express-static-gzip": "^0.3.2",
    "express-ws": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.67.1",
    "flow-typed": "^2.3.0",
    "graphql": "^0.13.1",
    "graphql-tools": "^2.21.0",
    "html-webpack-plugin": "^3.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "jest-environment-enzyme": "^5.0.3",
    "jest-enzyme": "^5.0.3",
    "lighthouse": "^2.9.1",
    "minimist": "^1.2.0",
    "module-concat": "^2.3.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "npm-run-all": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "pixelmatch": "^4.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-sass": "^0.3.0",
    "postcss-url": "^7.3.1",
    "prettier": "^1.11.1",
    "puppeteer": "^1.1.1",
    "react-devtools": "^3.1.0",
    "react-styleguidist": "^6.2.7",
    "redis": "^2.8.0",
    "redux-devtools": "^3.4.1",
    "sinon": "^4.4.2",
    "slug": "^0.9.1",
    "start-server-and-test": "^1.0.1",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-no-unsupported-browser-features": "^2.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-pwa-manifest": "^3.6.1",
    "webpack-subresource-integrity": "^1.1.0-rc.2",
    "zopfli-webpack-plugin": "^0.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "connected-react-router": "^4.3.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "nskeymirror": "^0.1.2",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "engines": {
    "node": "8.10.0",
    "npm": "5.7.1"
  }
}
